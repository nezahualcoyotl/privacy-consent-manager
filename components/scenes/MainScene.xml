<?xml version="1.0" encoding="utf-8" ?>
<component name="MainScene" extends="Scene">
    <interface>
        <field id="routerCallback" type="assocarray" onChange="routingEventCallback" alwaysNotify="true" />
        <field id="event" type="assocarray" alwaysNotify="true" />
        <field id="rootScene" type="boolean" />
    </interface>
    <script type="text/brightscript" uri="MainScene.brs" />
    <script type="text/brightscript" uri="Router.brs" />
    <children>
        <Poster id="background" uri="pkg:/images/background.jpg" width="1920" height="1080" />
        <Group id="scenes" />
    </children>
</component>